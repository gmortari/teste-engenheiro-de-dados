CREATE TABLE PARTICIPANTE (
	NU_INSCRICAO NUMERIC(12) NOT NULL,
	NU_ANO NUMERIC(4) NULL,
	TP_FAIXA_ETARIA NUMERIC(2) NULL,
	TP_SEXO char(1) NULL,
	TP_ESTADO_CIVIL NUMERIC(1) NULL,
	TP_COR_RACA NUMERIC(1) NULL,
	TP_NACIONALIDADE NUMERIC(1) NULL,
	TP_ST_CONCLUSAO NUMERIC(1) NULL,
	TP_ANO_CONCLUIU NUMERIC(2) NULL,
	TP_ESCOLA NUMERIC(1) NULL,
	TP_ENSINO NUMERIC(1) NULL,
	IN_TREINEIRO char(1) NULL,
	CO_MUNICIPIO_ESC NUMERIC(7) NULL,
	CO_MUNICIPIO_PROVA NUMERIC(7) NULL,
	PRIMARY KEY (NU_INSCRICAO)
);

CREATE TABLE ESCOLA (
	CO_MUNICIPIO_ESC NUMERIC(7) NOT NULL,
	NO_MUNICIPIO_ESC varchar(150) NULL,
	CO_UF_ESC NUMERIC(2) NULL,
	SG_UF_ESC char(2) NULL,
	TP_DEPENDENCIA_ADM_ESC NUMERIC(1) NULL,
	TP_LOCALIZACAO_ESC NUMERIC(1) NULL,
	TP_SIT_FUNC_ESC NUMERIC(1) NULL,
	PRIMARY KEY (CO_MUNICIPIO_ESC)
);

CREATE TABLE LOCAL_PROVA (
	CO_MUNICIPIO_PROVA NUMERIC(7) NOT NULL,
	NO_MUNICIPIO_PROVA varchar(150) NULL,
	CO_UF_PROVA char(2) NULL,
	SG_UF_PROVA char(2) NULL,
	PRIMARY KEY (CO_MUNICIPIO_PROVA)
);

CREATE TABLE PROVA_OBJETIVA (
	ID SERIAL,
	TP_PRESENCA_CN NUMERIC(1) NULL,
	TP_PRESENCA_CH NUMERIC(1) NULL,
	TP_PRESENCA_LC NUMERIC(1) NULL,
	TP_PRESENCA_MT NUMERIC(1) NULL,
	CO_PROVA_CN NUMERIC(3) NULL,
	CO_PROVA_CH NUMERIC(3) NULL,
	CO_PROVA_LC NUMERIC(3) NULL,
	CO_PROVA_MT NUMERIC(3) NULL,
	NU_NOTA_CN NUMERIC(9) NULL,
	NU_NOTA_CH NUMERIC(9) NULL,
	NU_NOTA_LC NUMERIC(9) NULL,
	NU_NOTA_MT NUMERIC(9) NULL,
	TX_RESPOSTAS_CN varchar(50) NULL,
	TX_RESPOSTAS_CH varchar(50) NULL,
	TX_RESPOSTAS_LC varchar(50) NULL,
	TX_RESPOSTAS_MT varchar(50) NULL,
	TP_LINGUA NUMERIC(1) NULL,
	TX_GABARITO_CN varchar(50) NULL,
	TX_GABARITO_CH varchar(50) NULL,
	TX_GABARITO_LC varchar(50) NULL,
	TX_GABARITO_MT varchar(50) NULL,
	NU_INSCRICAO NUMERIC(12) NOT NULL,
	PRIMARY KEY(ID)
);

CREATE TABLE REDACAO (
	ID SERIAL,
	TP_STATUS_REDACAO NUMERIC(1) NULL,
	NU_NOTA_COMP1 NUMERIC(9) NULL,
	NU_NOTA_COMP2 NUMERIC(9) NULL,
	NU_NOTA_COMP3 NUMERIC(9) NULL,
	NU_NOTA_COMP4 NUMERIC(9) NULL,
	NU_NOTA_COMP5 NUMERIC(9) NULL,
	NU_NOTA_REDACAO NUMERIC(9) NULL,
	NU_INSCRICAO NUMERIC(12) NOT NULL,
	PRIMARY KEY(ID)
);

CREATE TABLE QUESTIONARIO (
	ID SERIAL,
	Q001 char(1) NULL,
	Q002 char(1) NULL,
	Q003 char(1) NULL,
	Q004 char(1) NULL,
	Q005 NUMERIC(2) NULL,
	Q006 char(1) NULL,
	Q007 char(1) NULL,
	Q008 char(1) NULL,
	Q009 char(1) NULL,
	Q010 char(1) NULL,
	Q011 char(1) NULL,
	Q012 char(1) NULL,
	Q013 char(1) NULL,
	Q014 char(1) NULL,
	Q015 char(1) NULL,
	Q016 char(1) NULL,
	Q017 char(1) NULL,
	Q018 char(1) NULL,
	Q019 char(1) NULL,
	Q020 char(1) NULL,
	Q021 char(1) NULL,
	Q022 char(1) NULL,
	Q023 char(1) NULL,
	Q024 char(1) NULL,
	Q025 char(1) NULL,
	NU_INSCRICAO NUMERIC(12) NOT NULL,
	PRIMARY KEY(ID)
);

ALTER TABLE PARTICIPANTE ADD CONSTRAINT PARTICIPANTE_fk0 FOREIGN KEY (CO_MUNICIPIO_ESC) REFERENCES ESCOLA(CO_MUNICIPIO_ESC);

ALTER TABLE PARTICIPANTE ADD CONSTRAINT PARTICIPANTE_fk1 FOREIGN KEY (CO_MUNICIPIO_PROVA) REFERENCES LOCAL_PROVA(CO_MUNICIPIO_PROVA);

ALTER TABLE PROVA_OBJETIVA ADD CONSTRAINT PROVA_OBJETIVA_fk0 FOREIGN KEY (NU_INSCRICAO) REFERENCES PARTICIPANTE(NU_INSCRICAO);

ALTER TABLE REDACAO ADD CONSTRAINT REDACAO_fk0 FOREIGN KEY (NU_INSCRICAO) REFERENCES PARTICIPANTE(NU_INSCRICAO);

ALTER TABLE QUESTIONARIO ADD CONSTRAINT QUESTIONARIO_fk0 FOREIGN KEY (NU_INSCRICAO) REFERENCES PARTICIPANTE(NU_INSCRICAO);







